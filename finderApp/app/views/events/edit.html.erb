<h1>Editing Event</h1>
<% if user_signed_in? %>
    <%= render 'form', event: @event %>

    <%= link_to 'Show', @event %> |
    <%= link_to 'Back', events_path %>
<% else %>

    <p>you have to logged in </p>
    <%= link_to 'Back', event_path %>
    <%= link_to "Sign Up", new_user_registration_path %>
    <%= link_to "Sing in", new_user_session_path %>
<% end %>


